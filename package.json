{
  "name": "deadlock-stats",
  "version": "0.0.5",
  "private": true,
  "main": "index.ts",
  "license": "GPL-3.0",
  "scripts": {
    "android": "expo run:android --variant debugOptimized",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "bundle:web": "pnpx expo export --platform web",
    "serve:web": "pnpx serve dist",
    "prebuild:clean": "pnpx expo prebuild --clean",
    "compile": "tsc --noEmit -p . --pretty",
    "lint": "biome check .",
    "fmt": "biome format --write .",
    "align-deps": "pnpx expo install --fix",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build:ios:sim": "eas build --profile development --platform ios --local",
    "build:ios:dev": "eas build --profile development:device --platform ios --local",
    "build:ios:preview": "eas build --profile preview --platform ios --local",
    "build:ios:prod": "eas build --profile production --platform ios --local",
    "build:android:sim": "eas build --profile development --platform android --local",
    "build:android:dev": "eas build --profile development:device --platform android --local",
    "build:android:preview": "eas build --profile preview --platform android --local",
    "build:android:prod": "eas build --profile production --platform android --local"
  },
  "dependencies": {
    "@expo-google-fonts/space-grotesk": "^0.4.1",
    "@expo/metro-runtime": "~6.1.2",
    "@nandorojo/galeria": "^1.2.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@shopify/react-native-skia": "2.2.12",
    "@tanstack/react-query": "^5.90.5",
    "assets-deadlock-api-client": "github:deadlock-api/openapi-clients#path:/typescript/assets-api",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "deadlock-api-client": "github:deadlock-api/openapi-clients#path:/typescript/api",
    "expo": "^54.0.15",
    "expo-application": "~7.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-system-ui": "~6.0.7",
    "i18next": "^25.6.0",
    "intl-pluralrules": "^2.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.1.3",
    "react-native": "0.81.4",
    "react-native-android-widget": "^0.17.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-global-state-hooks": "^11.0.1",
    "react-native-keyboard-controller": "^1.18.5",
    "react-native-marked": "^7.0.2",
    "react-native-mmkv": "^4.0.0",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-sse": "^1.2.1",
    "react-native-svg": "^15.12.1",
    "react-native-turnstile": "^1.0.9",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "reanimated-tab-view": "^0.4.0",
    "use-debounce": "^10.0.6",
    "victory-native": "^41.20.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/react": "~19.2.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "intl-pluralrules",
          "react-native-global-state-hooks",
          "react-native-turnstile",
          "deadlock-api-client",
          "assets-deadlock-api-client"
        ]
      }
    }
  }
}
